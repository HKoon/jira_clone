{
  "services": {
    "api": {
      "app_dir": "./api",
      "build_command": "npm install && npm run build",
      "start_command": "npm start",
      "install_command": "npm install",
      "port": 5000
    },
    "client": {
      "app_dir": "./client",
      "build_command": "npm install && npm run build",
      "start_command": "npm run serve",
      "install_command": "npm install",
      "port": 3000
    }
  }
}